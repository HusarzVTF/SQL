

USE Stacja_Paliw

GO

IF OBJECT_ID('Ope.uspZwolnij') IS NOT NULL
DROP PROC Ope.uspZwolnij;

GO

CREATE PROC Ope.uspZwolnij (@idprac tinyint, @datazwolnienia date)

AS

UPDATE Ope.Pracownicy 
SET data_Zwolnienia = @datazwolnienia, status_pracownika ='zwolniony'
WHERE IDPracownika = @idprac 

/*procedura na zwolnienie pracownika*/

