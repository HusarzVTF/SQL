

USE Stacja_Paliw

GO

DECLARE	@return_value int

EXEC @return_value = Ope.uspWstawFV_Sprzedaz
		 @IdKlienta = 2,
		 @IdPracownika = 1,
		 @IDPaliwa = 1,
		 @IloscPaliwa = 1000,
		 @typrabatu = 'automatyczny',
		 @rabjedn = NULL,
		 @DataZaplaty = NULL,
		 @DataAktuZaplaty = NULL

SELECT	'Return Value' = @return_value

GO


DECLARE	@return_value int 

EXEC @return_value = Ope.uspWstawFV_Sprzedaz
		 @IdKlienta = 5,
		 @IdPracownika = 1,
		 @IDPaliwa = 2,
		 @IloscPaliwa = 1000.57,
		 @typrabatu = 'automatyczny',
		 @rabjedn = NULL,
		 @DataZaplaty = NULL,
		 @DataAktuZaplaty = NULL

SELECT	'Return Value' = @return_value

GO 

DECLARE	@return_value int

EXEC @return_value = Ope.uspWstawFV_Sprzedaz
		 @IdKlienta = 8,
		 @IdPracownika = 1,
		 @IDPaliwa = 3,
		 @IloscPaliwa = 16780,
		 @typrabatu = 'automatyczny',
		 @rabjedn = NULL,
		 @DataZaplaty = NULL,
		 @DataAktuZaplaty = NULL

SELECT	'Return Value' = @return_value

GO 

/* wystawienie fv sprzedazy po wprowadzeniu 0 podatku przez rzad i uruchomieniu systemu rabatowego*/
