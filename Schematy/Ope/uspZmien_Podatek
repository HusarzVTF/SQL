

USE Stacja_Paliw

GO


IF OBJECT_ID('Ope.uspZmien_Podatek') IS NOT NULL
DROP PROC Ope.uspZmien_Podatek;

GO

CREATE PROC Ope.uspZmien_Podatek (@rdz varchar(7), @stawka decimal(5,4))

AS

UPDATE Ope.StPodat SET stawka=@stawka WHERE ID=Ope.ufID_typ_Paliwa(@rdz)

/*Funkcja aktualizaujca stawke podatkowa na wskazany po nazwie rodzaj paliwa*/

